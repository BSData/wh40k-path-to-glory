          <modifiers/>
          <rules>
            <rule id="6455-9d36-c0fa-fb69" name="Acolytes of Darkness" hidden="false">
              <description>Once per battle, at the start of your turn, this unit can enact a dark ritual which grants them the Shrouded special rule until the start of your next turn. </description>
              <modifiers/>
            </rule>
          </rules>
          <profiles/>
          <links/>
